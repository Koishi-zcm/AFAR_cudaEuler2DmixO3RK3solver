FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

gaussQuadrature
{
    cOrder 7;
    fOrder 5;
}

CUDA
{
    R  0.7142857;
    Cv  1.7857142857;
    stencilType  edgeNeighbour;

    boundary
    {
        patch0  5;
        patch1  2;
        patch2  4;
    }
}

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         leastSquares;
}

divSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


